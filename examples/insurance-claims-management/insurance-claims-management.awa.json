    "decision_nodes": [
        {
            "id": "decision-routing",
            "name": "Claim Routing Decision",
            "description": "Routes claims based on amount, fraud score, and confidence. 95% of claims go through automated approval, 3% require high-value human review, 2% require exception/fraud investigation.",
            "decision_table": {
                "hit_policy": "first",
                "inputs": [
                    {
                        "name": "claim_amount",
                        "label": "Claim Amount ($)",
                        "type": "number"
                    },
                    {
                        "name": "fraud_score",
                        "label": "Fraud Risk Score",
                        "type": "number"
                    },
                    {
                        "name": "confidence_score",
                        "label": "AI Confidence",
                        "type": "number"
                    },
                    {
                        "name": "has_exception",
                        "label": "Has Exception Flag",
                        "type": "boolean"
                    }
                ],
                "outputs": [
                    {
                        "name": "route",
                        "label": "Routing Decision",
                        "type": "string",
                        "allowed_values": [
                            "auto_approve",
                            "human_highvalue",
                            "human_exception"
                        ]
                    }
                ],
                "rules": [
                    {
                        "id": "rule-fraud",
                        "description": "High fraud score or exception → Human exception review",
                        "input_entries": [
                            "-",
                            "> 70",
                            "-",
                            "-"
                        ],
                        "output_entries": [
                            "human_exception"
                        ],
                        "output_edge_id": "edge-9-exception"
                    },
                    {
                        "id": "rule-exception",
                        "description": "Exception flagged → Human exception review",
                        "input_entries": [
                            "-",
                            "-",
                            "-",
                            "true"
                        ],
                        "output_entries": [
                            "human_exception"
                        ],
                        "output_edge_id": "edge-9-exception"
                    },
                    {
                        "id": "rule-lowconfidence",
                        "description": "Low AI confidence → Human review",
                        "input_entries": [
                            "-",
                            "-",
                            "< 0.7",
                            "-"
                        ],
                        "output_entries": [
                            "human_highvalue"
                        ],
                        "output_edge_id": "edge-8-highvalue"
                    },
                    {
                        "id": "rule-highvalue",
                        "description": "High value claims → Human review",
                        "input_entries": [
                            "> 10000",
                            "< 70",
                            ">= 0.7",
                            "false"
                        ],
                        "output_entries": [
                            "human_highvalue"
                        ],
                        "output_edge_id": "edge-8-highvalue"
                    },
                    {
                        "id": "rule-auto",
                        "description": "Standard claims → Automated approval (95% of cases)",
                        "input_entries": [
                            "<= 10000",
                            "< 30",
                            ">= 0.85",
                            "false"
                        ],
                        "output_entries": [
                            "auto_approve"
                        ],
                        "output_edge_id": "edge-7-auto"
                    }
                ]
            },
            "default_output_edge_id": "edge-8-highvalue"
        }
    ],
