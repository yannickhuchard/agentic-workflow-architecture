{
    "name": "agentic-workflow-architecture",
    "version": "1.0.0",
    "description": "TypeScript SDK for Agentic Workflow Architecture (AWA)",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "bin": {
        "awa": "dist/cli/index.js"
    },
    "scripts": {
        "build": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts --watch",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "lint": "eslint src/",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "keywords": [
        "awa",
        "agentic",
        "workflow",
        "process",
        "bpmn",
        "ai-agent"
    ],
    "author": "AWA Specification",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yannickhuchard/agentic-workflow-architecture.git"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@prisma/client": "^6.19.2",
        "commander": "^14.0.3",
        "cors": "^2.8.6",
        "dotenv": "^17.2.4",
        "express": "^5.2.1",
        "uuid": "^9.0.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^20.0.0",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^8.56.0",
        "prisma": "^6.19.2",
        "tsup": "^8.0.0",
        "typescript": "^5.4.0",
        "vitest": "^1.0.0"
    },
    "peerDependencies": {
        "typescript": ">=5.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}